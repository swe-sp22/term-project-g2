<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" style="-fx-background-color: #000000; -fx-background-image: url('/com/images/2.jpg'); -fx-background-size: cover;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gui.editMember">
<children>
   <Button fx:id="cancelShowMembers" layoutX="997.0" layoutY="863.0" mnemonicParsing="false" onAction="#gotoMainScreen" prefHeight="36.0" prefWidth="125.0" style="-fx-background-color: #D65A31;" text="Cancel" textFill="WHITE">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
   <Button fx:id="saveMember" layoutX="816.0" layoutY="863.0" mnemonicParsing="false" onAction="#saveMember" prefHeight="36.0" prefWidth="126.0" style="-fx-background-color: #D65A31;" text="Save" textFill="WHITE" visible="false">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
   <Label layoutX="718.0" layoutY="80.0" prefHeight="70.0" prefWidth="483.0" text="Search For a Member" textFill="#d45731">
      <font>
         <Font name="System Bold" size="48.0" />
      </font>
   </Label>
      <TextField fx:id="searchText" layoutX="718.0" layoutY="222.0" prefHeight="70.0" prefWidth="350.0" promptText="Enter Member ID:" />
      <Button fx:id="searchButton" layoutX="1112.0" layoutY="221.0" mnemonicParsing="false" onAction="#searchMember" prefHeight="70.0" prefWidth="89.0" style="-fx-background-color: #D65A31;" text="Search" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label fx:id="EditLabel" layoutX="754.0" layoutY="344.0" text="Edit Member Data" textFill="#d45633" visible="false">
         <font>
            <Font name="System Bold" size="48.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="816.0" layoutY="471.0" prefHeight="43.0" prefWidth="307.0" promptText="Name" visible="false" />
      <TextField fx:id="phoneField" layoutX="816.0" layoutY="546.0" prefHeight="43.0" prefWidth="307.0" promptText="Phone Number" visible="false" />
      <TextField fx:id="MTField" layoutX="816.0" layoutY="701.0" prefHeight="43.0" prefWidth="307.0" promptText="Membership Type" visible="false" />
      <TextField fx:id="EID" layoutX="816.0" layoutY="785.0" prefHeight="43.0" prefWidth="307.0" promptText="Employee ID" visible="false" />
      <DatePicker fx:id="date" layoutX="816.0" layoutY="621.0" prefHeight="43.0" prefWidth="307.0" visible="false" />
      <Label fx:id="MIDnotfound" layoutX="734.0" layoutY="370.0" text="Member ID NOT FOUND" textFill="#ff0101" visible="false">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Label fx:id="savedsuccess" layoutX="855.0" layoutY="951.0" text="Saved Successfully" textFill="#d65a31" visible="false">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="saveerror" layoutX="847.0" layoutY="951.0" text="Error: Couldn't Save" textFill="RED" visible="false">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="emptyerror" layoutX="763.0" layoutY="960.0" text="Error: Don't Leave EMPTY fields" textFill="RED" visible="false">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
